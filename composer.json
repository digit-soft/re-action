{
    "name": "digit-soft/re-action",
    "version": "0.2.5",
    "description": "React PHP framework",
    "keywords": ["digit-soft", "reactphp", "framework", "react php framework"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/digit-soft/re-action/issues",
        "source": "https://github.com/digit-soft/re-action"
    },
    "authors": [
        {
            "name": "Digit",
            "email": "digit.vova@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.0",
        "doctrine/annotations": "~1.6",
        "react/http": "~0.8",
        "react/filesystem": "*",
        "digit-soft/re-action-event-emitter": "*",
        "digit-soft/re-action-promise": "*",
        "digit-soft/re-action-clients-pool": "*",
        "nikic/fast-route": "*",
        "psr/log": "*",
        "cebe/markdown": "~1.2",
        "ezyang/htmlpurifier": "~4.10",
        "voryx/pgasync": "~2.0",
        "jeremeamia/superclosure": "^2.4",
        "zumba/json-serializer": "~2",
        "bower-asset/jquery": "~3.2",
        "bower-asset/bootstrap": "~4.1",
        "bower-asset/popper.js": "~1.14",
        "bower-asset/punycode": "~2.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7"
    },
    "autoload": {
        "psr-4": {
            "Reaction\\": "src/"
        },
        "files": [ "src/Reaction.php" ]
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
